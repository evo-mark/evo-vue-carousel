{
    "name": "@evomark/evo-vue-carousel",
    "publishConfig": {
        "access": "restricted"
    },
    "version": "1.0.1",
    "type": "module",
    "files": [
        "dist"
    ],
    "exports": {
        ".": {
            "development": {
                "require": "./src/main.js",
                "import": "./src/main.js"
            },
            "default": {
                "require": "./dist/evo-vue-carousel.js",
                "import": "./dist/evo-vue-carousel.js"
            }
        },
        "./tailwind": {
            "require": "./dist/node-tailwind.js",
            "import": "./dist/node-tailwind.js"
        },
        "./tailwind-dev": {
            "require": "./src/node/tailwind.js",
            "import": "./src/node/tailwind.js"
        }
    },
    "scripts": {
        "dev": "vite",
        "build": "vite build && cp ./src/node/tailwind.js ./dist/node-tailwind.js",
        "preview": "vite preview ",
        "demo": "pnpm --filter='./demo' run dev",
        "demo:build": "pnpm --filter='./demo' run build",
        "demo:preview": "pnpm --filter='./demo' run preview",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@eslint/js": "^9.11.1",
        "@vitejs/plugin-vue": "^5.1.4",
        "eslint": "^9.11.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-jsdoc": "^50.2.4",
        "eslint-plugin-vue": "^9.28.0",
        "globals": "^15.9.0",
        "prettier": "^3.3.3",
        "tailwind-merge": "^2.5.2",
        "tailwindcss": "^3.4.13",
        "vite": "^5.4.8",
        "vite-plugin-lib-inject-css": "^2.1.1",
        "vue": "^3.5.8"
    },
    "peerDependencies": {
        "tailwind-merge": "^2.5.2",
        "vue": "^3.4.38"
    },
    "packageManager": "pnpm@9.11.0+sha512.0a203ffaed5a3f63242cd064c8fb5892366c103e328079318f78062f24ea8c9d50bc6a47aa3567cabefd824d170e78fa2745ed1f16b132e16436146b7688f19b",
    "dependencies": {
        "@evomark/vue-forward-slots": "^6.2.0",
        "@skirtle/vue-vnode-utils": "^0.1.4",
        "@vueuse/core": "^11.1.0",
        "lodash-es": "^4.17.21",
        "vue3-icon": "^3.0.3"
    }
}